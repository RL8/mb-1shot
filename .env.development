# Development Environment Variables - SINGLE SOURCE OF TRUTH

# Frontend Configuration (VITE_ prefix for Vite)
VITE_API_URL=http://localhost:3001
VITE_APP_NAME=Music Besties
VITE_APP_VERSION=1.0.0
VITE_APP_ENV=development

# Backend Configuration
PORT=3001
NODE_ENV=development
FRONTEND_URL=http://localhost:3000

# OpenAI Configuration - Set these in Vercel environment variables
OPENAI_API_KEY=your_openai_api_key_here

# AuraDB Configuration (Neo4j) - Set these in Vercel environment variables
AURA_DB_URI=your_aura_db_uri_here
AURA_DB_USERNAME=your_aura_db_username_here
AURA_DB_PASSWORD=your_aura_db_password_here

# AG-UI Configuration
AGUI_ENABLED=true
AGUI_WS_PORT=3001

# Security & CORS
JWT_SECRET=dev-jwt-secret-change-in-production
CORS_ORIGINS=http://localhost:3000

# Spotify Integration - Set these in Vercel environment variables
SPOTIFY_CLIENT_ID=your_spotify_client_id_here
SPOTIFY_CLIENT_SECRET=your_spotify_client_secret_here 